{
    "name": "vanthao03596/laravel-ghtk",
    "description": "This is my package LaravelGhtk",
    "keywords": [
        "vanthao03596",
        "laravel",
        "laravel-ghtk"
    ],
    "homepage": "https://github.com/vanthao03596/laravel-ghtk",
    "license": "MIT",
    "authors": [
        {
            "name": "phamthao",
            "email": "phamthao03596@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "graham-campbell/manager": "^4.5",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "vanthao03596/ghtk-sdk": "^0.0.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^4.8|^5.2|^6.0",
        "phpunit/phpunit": "^9.3",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.0",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "Vanthao03596\\LaravelGhtk\\": "src",
            "Vanthao03596\\LaravelGhtk\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanthao03596\\LaravelGhtk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vanthao03596\\LaravelGhtk\\LaravelGhtkServiceProvider"
            ],
            "aliases": {
                "LaravelGhtk": "GhtkFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
